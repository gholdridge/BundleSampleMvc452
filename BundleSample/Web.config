<configuration>

  <system.web>
    <compilation debug="true" targetFramework="4.5.2"/>
    <httpRuntime targetFramework="4.5.2"/>
  </system.web>

  <appSettings>
    <!-- Testing Octopus Deploy setting variables. When a key matches an Octupus variable name, then Octopus automatically changes the value based on the environment it is deploying to. -->
    <add key="BundleSample.Environment" value="Development"/>
    <add key="Octopus.Release.Number" value="?.?.?.?"/>
  </appSettings>

</configuration>
